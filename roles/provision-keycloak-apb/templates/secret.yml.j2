apiVersion: v1
kind: Secret
metadata:
  name: {{ keycloak_secret_name }}
  namespace: {{ namespace }}
  labels:
    name: keycloak
    mobile: enabled
    serviceName: keycloak
    serviceInstanceID: "{{ _apb_service_instance_id }}"
stringData:
  user_id: "{{ ADMIN_USERNAME }}"
  user_name: "{{ ADMIN_USERNAME }}"
  user_passwd: "{{ ADMIN_PASSWORD }}"