apiVersion: v1
kind: ConfigMap
metadata:
  name: '{{ keycloak_configmap_name }}'
  namespace: {{ namespace }}
  labels:
    mobile: enabled
    serviceName: keycloak
    serviceInstanceID: {{ _apb_service_instance_id }}
data:
  type: keycloak
  realm: {{ KEYCLOAK_REALM }}
  name: keycloak
  uri: {{ KEYCLOAK_HOST }}
